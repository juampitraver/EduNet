@inject TP3.Core.Interfaces.IAuthorizationService authorizationService
<!DOCTYPE html>
<html lang="es">
<!-- begin::Head -->
<head>
    <meta charset="utf-8" />
    <title>Edu-Net</title>
    <meta name="description" content="Layout without header  menu">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href="~/css/select2/select2.min.css" rel="stylesheet" type="text/css" />
    <!--begin::Fonts -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700|Roboto:300,400,500,600,700">
    <link rel="stylesheet" href="~/css/fonts/flaticon/flaticon.css">
    <link rel="stylesheet" href="~/css/fonts/flaticon2/flaticon.css">
    <link rel="stylesheet" href="~/css/fonts/line-awesome/css/line-awesome.css">
    <link rel="stylesheet" href="~/css/fonts/fontAwesome/css/all.min.css">
    <!--end::Fonts -->
    <!--begin:: Global Mandatory Vendors -->
    <link href="~/css/layout/perfectScrollbar/perfect-scrollbar.min.rtl.css" rel="stylesheet" type="text/css" />
    <link href="~/css/layout/toastr/toastr.min.css" rel="stylesheet" type="text/css" />
    <!--end:: Global Mandatory Vendors -->
    <!--begin::Global Theme Styles(used by all pages) -->
    <link href="~/css/layout/style.bundle.min.css" rel="stylesheet" type="text/css" />
    <!--end::Global Theme Styles -->
    <!--begin::Layout Skins(used by all pages) -->
    <link href="~/css/layout/header/light.min.css" rel="stylesheet" type="text/css" />
    <link href="~/css/layout/menu/light.min.css" rel="stylesheet" type="text/css" />
    <link href="~/css/layout/brand/light.min.css" rel="stylesheet" type="text/css" />
    <link href="~/css/layout/aside/light.min.css" rel="stylesheet" type="text/css" />
    <link href="~/css/site.css" rel="stylesheet" type="text/css" />
    <!--end::Layout Skins -->
    @RenderSection("Styles", required: false)
    <link rel="shortcut icon" href="~/images/logo/favicon.ico" />
</head>
<body class="kt-quick-panel--right kt-demo-panel--right kt-offcanvas-panel--right kt-header--fixed kt-header-mobile--fixed kt-subheader--enabled kt-subheader--transparent kt-aside--enabled kt-aside--fixed kt-aside--minimize kt-page--loading">
    <!-- begin:: Page -->
    <!-- begin:: Header Mobile -->
    <div id="kt_header_mobile" class="kt-header-mobile  kt-header-mobile--fixed ">
        <div class="kt-header-mobile__logo">
            <a asp-action="Login" asp-controller="Account">
                <img alt="Logo" src="~/images/logo/menuLogo.png" />
            </a>
        </div>
        <div class="kt-header-mobile__toolbar">
            <button class="kt-header-mobile__toolbar-toggler kt-header-mobile__toolbar-toggler--left" id="kt_aside_mobile_toggler"><span></span></button>
            <button class="kt-header-mobile__toolbar-toggler" id="kt_header_mobile_toggler"><span></span></button>
            <button class="kt-header-mobile__toolbar-topbar-toggler" id="kt_header_mobile_topbar_toggler"><i class="flaticon-more"></i></button>
        </div>
    </div>
    <!-- end:: Header Mobile -->
    <div class="kt-grid kt-grid--hor kt-grid--root">
        <div class="kt-grid__item kt-grid__item--fluid kt-grid kt-grid--ver kt-page">
            <!-- begin:: Aside -->
            <button class="kt-aside-close " id="kt_aside_close_btn"><i class="la la-close"></i></button>

            <div class="kt-aside  kt-aside--fixed  kt-grid__item kt-grid kt-grid--desktop kt-grid--hor-desktop" id="kt_aside">
                <!-- begin:: Brand -->
                <div class="kt-aside__brand kt-grid__item " id="kt_aside_brand">
                    <div class="kt-aside__brand-logo">
                        <a asp-action="Login" asp-controller="Account">
                            <img alt="Logo" src="~/images/logo/menuLogo.png" />
                        </a>
                    </div>
                </div>
                <!-- end:: Brand -->	<!-- begin:: Aside Menu -->
               <partial name="_Menu">
                <!-- end:: Aside Menu -->
            </div>
            <div class="kt-aside-menu-overlay"></div>
            <!-- end:: Aside -->
            <div class="kt-grid__item kt-grid__item--fluid kt-grid kt-grid--hor kt-wrapper" id="kt_wrapper">
                <!-- begin:: Header -->
                <div id="kt_header" class="kt-header kt-grid kt-grid--ver  kt-header--fixed ">
                    <!-- begin: Header Menu -->
                    <div class="kt-header-menu-wrapper kt-grid__item kt-grid__item--fluid" id="kt_header_menu_wrapper">
                        
                    </div>
                    <!-- end: Header Menu -->
                    <!-- begin:: Header Topbar -->
                    <div class="kt-header__topbar">
                        <!--begin: Search -->
                        <div class="kt-header__topbar-item kt-header__topbar-item--search">
                            @DateTime.Now.ToLongDateString()
                        </div>
                        <!--end: Search -->
                        <!--begin: Notifications -->
                        <div class="kt-header__topbar-item dropdown">
                            <div class="kt-header__topbar-wrapper" data-toggle="dropdown" data-offset="10px,0px">

                            </div>
                        </div>
                        <!--end: Notifications -->
                        <!--begin: Quick actions -->
                        <div class="kt-header__topbar-item dropdown">
                            <div class="kt-header__topbar-wrapper" data-toggle="dropdown" data-offset="10px,0px">

                            </div>
                        </div>
                        <!--end: Quick actions -->
                        <!--begin: Cart -->
                        <div class="kt-header__topbar-item dropdown">
                            <div class="kt-header__topbar-wrapper" data-toggle="dropdown" data-offset="10px,0px">

                            </div>
                        </div>
                        <!--end: Cart-->
                        <!--begin: User bar -->
                        @if (User.Identity.IsAuthenticated)
                        {
                            <div class="kt-header__topbar-item kt-header__topbar-item--user">
                                <div class="kt-header__topbar-wrapper" data-toggle="dropdown" data-offset="10px,0px">
                                    <span class="kt-hidden kt-header__topbar-welcome">Hola,</span>
                                    <span class="kt-hidden kt-header__topbar-username">@User.Identity.Name</span>
                                    <img class="kt-hidden" alt="Pic" src="./assets/media/users/300_21.jpg" />
                                    <span class="kt-header__topbar-icon"><i class="flaticon2-user-outline-symbol"></i></span>
                                </div>
                                <div class="dropdown-menu dropdown-menu-fit dropdown-menu-right dropdown-menu-anim dropdown-menu-xl">
                                    <!--begin: Head -->
                                    <div class="kt-user-card kt-user-card--skin-light kt-notification-item-padding-x">
                                        <div class="kt-user-card__avatar">
                                            <!--use below badge element instead the user avatar to display username's first letter(remove kt-hidden class to display it) -->
                                            <span class="kt-badge kt-badge--username kt-badge--unified-success kt-badge--lg kt-badge--rounded kt-badge--bold kt-hidden">S</span>
                                        </div>
                                        <div class="kt-user-card__name">
                                            @User.Identity.Name
                                        </div>
                                    </div>
                                    <!--end: Head -->
                                    <!--begin: Navigation -->
                                    <div class="kt-notification">
                                        <div class="kt-notification__custom kt-space-between">
                                            <a asp-action="Logout" asp-controller="Account"  class="btn btn-label btn-label-brand btn-sm btn-bold">Cerrar Sesión</a>
                                        </div>
                                    </div>
                                    <!--end: Navigation -->
                                </div>
                            </div>
                        }
                        <!--end: User bar -->
                    </div>
                    <!-- end:: Header Topbar -->
                </div>
                <!-- end:: Header -->
                <div class="kt-content  kt-grid__item kt-grid__item--fluid kt-grid kt-grid--hor" id="kt_content">
                    <!-- begin:: Content -->
                    <div class="kt-container  kt-container--fluid  kt-grid__item kt-grid__item--fluid">
                        @RenderBody()
                    </div>
                    <!-- end:: Content -->
                </div>
                <!-- begin:: Footer -->
                <div class="kt-footer  kt-grid__item kt-grid kt-grid--desktop kt-grid--ver-desktop" id="kt_footer">
                    <div class="kt-container  kt-container--fluid ">
                        <div class="kt-footer__copyright">
                            2020&nbsp;&copy;&nbsp;<a href="#" class="kt-link">Tecnología Educativa</a>
                        </div>
                        <div class="kt-footer__menu">
                        </div>
                    </div>
                </div>
                <!-- end:: Footer -->
            </div>
        </div>
    </div>
    <!-- end:: Page -->
    <!-- begin::Global Config(global config for global JS sciprts) -->
    <script>
        var KTAppOptions = { "colors": { "state": { "brand": "#22b9ff", "light": "#ffffff", "dark": "#282a3c", "primary": "#5867dd", "success": "#34bfa3", "info": "#36a3f7", "warning": "#ffb822", "danger": "#fd3995" }, "base": { "label": ["#c5cbe3", "#a1a8c3", "#3d4465", "#3e4466"], "shape": ["#f0f3ff", "#d9dffa", "#afb4d4", "#646c9a"] } } };
    </script>
    <!-- end::Global Config -->
        <!--begin:: Global Mandatory Vendors -->
    <script src="~/js/layout/jquery.min.js" type="text/javascript"></script>
    <script src="~/js/layout/popper.min.js" type="text/javascript"></script>
    <script src="~/js/layout/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/js/layout/js.cookie.js" type="text/javascript"></script>
    <script src="~/js/layout/moment.min.js" type="text/javascript"></script>
    <script src="~/js/layout/tooltip.min.js" type="text/javascript"></script>
    <script src="~/js/layout/perfect-scrollbar.min.js" type="text/javascript"></script>
    <script src="~/js/layout/sticky.min.js" type="text/javascript"></script>
    <script src="~/js/layout/wNumb.js" type="text/javascript"></script>
    <script src="~/js/layout/toastr/toastr.min.js" type="text/javascript"></script>
    <script src="~/js/layout/jquery.blockUI.js" type="text/javascript"></script>
    <!--end:: Global Mandatory Vendors -->
    <!--begin::Global Theme Bundle(used by all pages) -->
    <script src="~/js/layout/scripts.bundle.min.js" type="text/javascript"></script>
    <!--end::Global Theme Bundle -->
    <!--begin::Jquery validate -->
    <script src="~/js/layout/jqueryValidate/jquery.validate.min.js" type="text/javascript"></script>
    <script src="~/js/layout/jqueryValidate/jquery.validate.unobtrusive.min.js" type="text/javascript"></script>
    <script src="~/js/layout/jqueryValidate/localization/messages_es_AR.min.js" type="text/javascript"></script>
    <!--end::Jquery validate -->
    <!--begin::CRUD notification -->
    <script>
        toastr.options = {
            "closeButton": false,
            "debug": false,
            "newestOnTop": false,
            "progressBar": true,
            "positionClass": "toast-top-center",
            "preventDuplicates": false,
            "onclick": null,
            "showDuration": "300",
            "hideDuration": "1000",
            "timeOut": "5000",
            "extendedTimeOut": "1000",
            "showEasing": "swing",
            "hideEasing": "linear",
            "showMethod": "fadeIn",
            "hideMethod": "fadeOut"
        };
        @if (TempData["RESPONSE"] != null)
        {
            var response = TempData.Get<TP3.Core.Data.BaseData.ResponseData>("RESPONSE");
            <text>
                var message = "@response.Message";
            </text>
            if (response.Result)
            {
                <text>
                 toastr.success(message);
                </text>
            }
            else
            {
                <text>
                toastr.error(message);
                 </text>
            }
        }
    </script>
    <script>
        $(document).ready(function () {
            autoCompleteOff();
            parceDecimal();
        });

        function parceDecimal() {
            $("input.decimal").change(function (event) {
                $(this).val($(this).val().replace(".", ","));
            });
        }
        function autoCompleteOff() {
            $(document).on('focus', ':input', function () {
                $(this).attr('autocomplete', 'off');
            });
        }
    </script>
    <!--end::CRUD notification -->
    @RenderSection("Scripts", required: false)
</body>
<!-- end::Body -->
</html>